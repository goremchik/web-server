{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "ui",
      "image": "490647521811.dkr.ecr.eu-central-1.amazonaws.com/app-ui",
      "hostname": "ui",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]
    },
    {
      "name": "api",
      "image": "490647521811.dkr.ecr.eu-central-1.amazonaws.com/app-api",
      "hostname": "api",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ]
    }
  ]
}
